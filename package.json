{
  "name": "hello-world-cweb-dapp",
  "packageManager": "yarn@4.3.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:cm && yarn build:non-cm && yarn create-index",
    "build:cm": "yarn workspaces foreach -Ap --include 'packages/*.cm' run build",
    "build:non-cm": "yarn workspaces foreach -Ap --include 'packages/*' --exclude 'packages/*.cm' run build",
    "create-index": "cweb-tool create-index .cweb-config/dapp-ecosystem.yaml .dapp-ecosystem-lock.yaml -c ./.cweb-config/config.json",
    "deploy-contracts": "yarn publish-actions",
    "call-contracts": "yarn call-contracts:prepare && yarn call-contracts:invoke",
    "call-contracts:prepare": "yarn workspaces foreach --all run prepare-for-package && yarn node ./.cweb-config/create-calls.mjs",
    "call-contracts:invoke": "cweb-tool call .cweb-config/calls.yaml -c ./.cweb-config/config.json",
    "execute-actions": "cweb-tool execute-actions ./.actions.yaml -c ./.cweb-config/config.json",
    "filter-actions": "cweb-tool filter-actions ./.unfiltered-actions.yaml .actions.yaml -c ./.cweb-config/config.json",
    "gather-actions": "cweb-tool gather-actions .dapp-ecosystem-lock.yaml .unfiltered-actions.yaml -c ./.cweb-config/config.json",
    "publish-actions": "cweb-tool publish .cweb-config/dapp-ecosystem.yaml -c ./.cweb-config/config.json",
    "clean": "yarn clean:me && yarn workspaces foreach -Ap run clean",
    "clean:me": "rimraf .dapp-ecosystem-lock.yaml .unfiltered-actions.yaml .actions.yaml",
    "test": "yarn workspaces foreach -Ap run test",
    "setup-ide:vscode": "yarn dlx @yarnpkg/sdks vscode",
    "install:with-node-modules": "yarn config set nodeLinker node-modules && yarn install",
    "preinstall": "node ./scripts/preinstall.env.mjs"
  },
  "resolutions": {
    "@coinweb/cweb-tool": "1.0.39",
    "@coinweb/contract-kit": "0.1.11",
    "@coinweb/self-register": "0.1.4",
    "@coinweb/wallet-lib": "0.1.113-esm"
  },
  "devDependencies": {
    "@coinweb/cweb-tool": "1.0.40",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "write-yaml-file": "^5.0.0"
  },
  "version": "0.0.2"
}
